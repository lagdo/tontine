{
    "name": "lagdo/tontine",
    "type": "project",
    "description": "An online tontine management application.",
    "keywords": ["tontine"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-gmp": "*",
        "ext-json": "*",
        "akaunting/laravel-money": "^3.1",
        "chrome-php/chrome": "^1.6",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jaxon-php/jaxon-annotations": "^2.1",
        "jaxon-php/jaxon-core": "^4.0",
        "jaxon-php/jaxon-dialogs": "^4.0",
        "jaxon-php/jaxon-laravel": "^4.0",
        "laravel/fortify": "^1.13",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.3",
        "mcamara/laravel-localization": "^1.7",
        "propaganistas/laravel-phone": "^4.3",
        "rinvex/countries": "^9.0",
        "spatie/laravel-validation-rules": "^3.2",
        "umpirsky/country-list": "^2.0",
        "umpirsky/currency-list": "^1.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "laravel-lang/lang": "^10.2",
        "laravel-lang/publisher": "^12.2",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Siak\\Tontine\\": "src/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php -r \"is_dir('storage/annotations') || mkdir('storage/annotations');\""
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
