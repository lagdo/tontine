{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-gmp": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "akaunting/laravel-money": "^5.2",
        "andreaselia/analytics": "^2.0",
        "chrome-php/chrome": "^1.12",
        "jaxon-php/jaxon-attributes": "^1.1",
        "jaxon-php/jaxon-core": "^5.6",
        "jaxon-php/jaxon-dialogs": "^5.1",
        "jaxon-php/jaxon-laravel": "^5.0",
        "lagdo/laravel-facades": "^1.0",
        "laravel/fortify": "^1.25",
        "laravel/framework": "^11.31",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/iso3166": "^4.3",
        "mcamara/laravel-localization": "^2.2",
        "mydnic/laravel-kustomer": "^1.10",
        "propaganistas/laravel-phone": "^5.3",
        "rinvex/countries": "^9.0",
        "spatie/laravel-html": "^3.11",
        "spatie/laravel-validation-rules": "^3.4",
        "sqids/sqids": "^0.4.1",
        "umpirsky/country-list": "^2.0",
        "umpirsky/currency-list": "^1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel-lang/attributes": "^2.11",
        "laravel-lang/http-statuses": "^3.8",
        "laravel-lang/lang": "^15.11",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Ajax\\": "ajax/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Siak\\Tontine\\": "src/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan lang:update"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
